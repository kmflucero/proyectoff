<h2>¿Cual es tu dirección de correo electronico?</h2>

<%= form_with(model: User.new, url: user_session_path) do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div><br>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div><br>
  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>
